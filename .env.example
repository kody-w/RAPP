# Environment Variables Template for Copilot Agent 365
# Copy this file to .env and fill in your actual credentials
# IMPORTANT: Never commit your actual .env file to version control

# Azure Storage Configuration
AZURE_STORAGE_ACCOUNT_NAME=your-storage-account-name
AZURE_STORAGE_ACCOUNT_KEY=your-storage-account-key
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=your-account;AccountKey=your-key;EndpointSuffix=core.windows.net

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your-azure-openai-key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_VERSION=2025-01-01-preview
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-5-chat
AZURE_AI_API_KEY=your-azure-ai-key

# Azure Image API Configuration
AZURE_IMAGE_API_ENDPOINT=https://your-resource.cognitiveservices.azure.com
AZURE_IMAGE_API_KEY=your-image-api-key
AZURE_IMAGE_DEPLOYMENT=gpt-image-1
AZURE_IMAGE_API_VERSION=2025-04-01-preview

# Azure DALL-E Configuration
AZURE_DALLE_API_KEY=your-dalle-api-key
AZURE_DALLE_ENDPOINT=https://your-dalle.openai.azure.com
AZURE_DALLE_API_VERSION=2024-02-01
AZURE_DALLE_DEPLOYMENT=Dalle3

# Dynamics 365 Configuration
DYNAMICS_365_CLIENT_ID=your-client-id
DYNAMICS_365_CLIENT_SECRET=your-client-secret
DYNAMICS_365_TENANT_ID=your-tenant-id
DYNAMICS_365_RESOURCE=https://your-org.crm.dynamics.com/

# SharePoint Configuration
SHAREPOINT_TENANT_ID=your-tenant-id
SHAREPOINT_CLIENT_ID=your-client-id
SHAREPOINT_CLIENT_SECRET=your-client-secret

# Microsoft Graph Configuration
GRAPH_CLIENT_ID=your-client-id
GRAPH_CLIENT_SECRET=your-client-secret
GRAPH_TENANT_ID=your-tenant-id

# Salesforce Configuration
SALESFORCE_CLIENT_ID=your-salesforce-client-id
SALESFORCE_CLIENT_SECRET=your-salesforce-client-secret
SALESFORCE_USERNAME=your-salesforce-username
SALESFORCE_PASSWORD=your-salesforce-password
SALESFORCE_SECURITY_TOKEN=your-salesforce-token
SALESFORCE_INSTANCE_URL=https://your-instance.my.salesforce.com

# ServiceNow Configuration
SERVICENOW_INSTANCE_URL=https://your-instance.service-now.com
SERVICENOW_USERNAME=your-servicenow-username
SERVICENOW_PASSWORD=your-servicenow-password

# Power Automate Configuration
EMAIL_DRAFTING_POWER_AUTOMATE_URL=your-power-automate-url
MESSAGE_ANALYZER_POWER_AUTOMATE_URL=your-power-automate-url

# Brave Search Configuration
BRAVE_SEARCH_API_KEY=your-brave-search-key

# Playwright Service Configuration
PLAYWRIGHT_SERVICE_ACCESS_TOKEN=your-playwright-token
PLAYWRIGHT_SERVICE_URL=https://eastus.playwright.microsoft.com/
PLAYWRIGHT_CONNECTION_ID=your-connection-id

# Azure Service Principal Configuration
AZURE_CLIENT_ID=your-service-principal-client-id
AZURE_TENANT_ID=your-tenant-id
AZURE_CLIENT_SECRET=your-service-principal-secret

# Application Configuration
ASSISTANT_NAME=Universal Synthetic Data AI
AZURE_FILES_SHARE_NAME=your-file-share-name
FUNCTIONS_WORKER_RUNTIME=python

# Azure Project Configuration
PROJECT_ENDPOINT=https://your-project.services.ai.azure.com/api/projects/your-project
CODE_REVIEW_PROJECT_ENDPOINT=https://your-project.services.ai.azure.com/api/projects/your-project
CODE_REVIEW_MODEL_DEPLOYMENT=gpt-4.1

# Application Insights Configuration
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=your-key;IngestionEndpoint=your-endpoint;LiveEndpoint=your-endpoint;ApplicationId=your-app-id

# GitHub Configuration
GITHUB_REPO_OWNER=your-github-username
GITHUB_REPO_NAME=your-repo-name
GITHUB_BRANCH=main
GITHUB_AGENTS_PATH=agents

# Agent Configuration
EXISTING_AGENT_ID=your-agent-id
DEFAULT_AGENT_ID=your-agent-id
DEFAULT_THREAD_ID=your-thread-id
